{
  "host": "{sample.host}",
  "readOnlyFileShareMounts": [
    {
      "FileShareName": "sample-fuzz",
      "MountPath": "/job-run"
    }
  ],
  "tasks": [
    {
      "toolName": "RESTler",
      "toolConfiguration": {
        "outputFolder": "RESTler-replay",
        "task": "Replay",
        "runConfiguration": {
          "inputFolderPath": "/job-run/{fuzz.jobId}/fuzz-1"
        }
      }
    }
  ]
}
