{
  "duration": "00:10:00",
  "rootFileshare": "sample",
  "webhook": {
    "name": "sample-fuzz"
  },
  "readOnlyFileShareMounts": [
    {
      "FileShareName": "sample",
      "MountPath": "/compile-out"
    }
  ],
  "testTasks": {
    "targetConfiguration" : {
      "host": "{sample.host}"
    },
    "tasks": [
      {
        "toolName": "RESTler",
        "outputFolder": "RESTler-fuzz-1",
        "toolConfiguration": {
          "task": "Fuzz",
          "runConfiguration": {
            "inputFolderPath": "/compile-out/{compile.jobId}/RESTler-compile-1"
          }
        }
      },
      {
        "toolName": "RESTler",
        "outputFolder": "RESTler-fuzz-2",
        "toolConfiguration": {
          "task": "Fuzz",
          "runConfiguration": {
            "inputFolderPath": "/compile-out/{compile.jobId}/RESTler-Compile-1"
          }
        }
      },
      {
        "toolName": "RESTler",
        "outputFolder": "RESTler-fuzz-3",
        "toolConfiguration": {
          "task": "FuzzRandomWalk",
          "runConfiguration": {
            "inputFolderPath": "/compile-out/{compile.jobId}/RESTler-compile-2"
          }
        }
      }
    ]
  }
}
