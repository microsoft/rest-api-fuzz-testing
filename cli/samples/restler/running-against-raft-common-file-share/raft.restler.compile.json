{
  "swaggerLocations": [{
    "Url": "https://{defaults.deploymentName}-raft-apiservice.azurewebsites.net/swagger/v1/swagger.json"
  }],
  "rootFileShare": "raft",
  "tasks": [
    {
      "toolName": "RESTler",
      "outputFolder": "compile",
      "toolConfiguration": {
        "task": "compile",
        "compileConfiguration": {
          "useRefreshableToken": true
        }
      }
    }
  ]
}
