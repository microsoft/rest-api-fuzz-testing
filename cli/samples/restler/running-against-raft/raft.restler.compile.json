{
  "testTasks": {
    "testTargetConfiguration" : {
      "apiSpecifications": [
        "https://{defaults.deploymentName}-raft-apiservice.azurewebsites.net/swagger/v1/swagger.json"
      ]
    },
    "tasks": [
      {
        "toolName": "RESTler",
        "outputFolder": "RESTler-compile",

        "toolConfiguration": {
          "task": "compile",
          "compileConfiguration": {
            "useRefreshableToken": true,
            "customDictionary": {
              "customPayload": {
                "cores" : ["1"],
                "memoryGBs" : ["1"],
                "toolName" : ["ZAP"],
                "duration": [ "00:10:00" ],
                "apiSpecifications": [ "https://some.service.azurewebsites.net/swagger.json", "/swagger-folder/swagger.json" ],
                "authenticationMethod": [ "{\"CommandLine\": \"abc\" }" ],
                "targetUrl": [ "{replace-with-valid-webhook-URL}" ],
                "event": [ "JobStatus" ],
                "timeSpanFilter": [ "00:10:00" ],
                "expectedDuration": [ "00:10:00" ],
                "eventName": [ "JobStatus" ],
                "region" : ["eastus2"]
              }
            }
          }
        }
      }
    ]
  }
}
